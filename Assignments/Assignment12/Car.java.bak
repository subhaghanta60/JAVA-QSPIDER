class Main
{
void Cost(int x){

System.out.println("The Cost of Car = "+ x);
}
void Cost(Double y){

System.out.println("The Cost of Car = "+y);
}

void Cost(int x,String y){

System.out.println("The Cost of Car = "+x +y);
}
void Cost(String x,int y){

System.out.println("The Cost of Car = "+y+x);
}
}
class Car
{
public static void main(String [] args)
{
Main M1=new Main();
M1.Cost(10);
M1.Cost(10.00);
M1.Cost(10,"Per one Quantity");
M1.Cost("Per one Quantity",10);
}
}