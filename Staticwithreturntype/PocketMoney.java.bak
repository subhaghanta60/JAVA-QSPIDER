class FairTicket
{
static double ticket()
{
double bus_ticket_price=15.00;
double taxi_ticket_price=20.00;

double Ticket_price=bus_ticket_price + taxi_ticket_price;

return Ticket_price;
}
}

class PocketMoney
{
public static void main(String [] args)
{
